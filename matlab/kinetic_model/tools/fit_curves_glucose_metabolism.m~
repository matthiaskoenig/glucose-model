function [f] = fit_curves_glucose_metabolism()

    f.z1 = @hgp_hgu;
    f.z2 = @gng_gly;
    f.z3 = @glyl_glys;

    % x (glucose), y (glycogen)
    %HGP and HGU
    function [z] = hgp_hgu(x, y)
        C = [   0.002037960420379  -0.000367490977632  -0.069301032419012  ... 
               -0.000002823120484   0.011282864074433   3.740276159346358  ...
               -0.000000181515364   0.000157328485157   -0.100050917438436 ...
               -18.414978834613287];
        z = C(1)*x^3 + C(2)*x^2*y + C(3)*x^2 + C(4)*x*y^2 + C(5)*x*y ...
            +C(6)*x + C(7)*y^3 + C(8)*y^2 + C(9)*y + C(10);
        return
    end

    %GNG and GLY
    function [z] = gng_gly(x, y)
        C = [  -0.013260401508103  -0.000078240970095   0.478235644004833 ...
                0.000002861605817   0.000932752106971  -2.492569641130055   0.000000166945924  -0.000125285017396
  Columns 9 through 10
   0.015354944655784  -4.975026288067225];
        z = C(1)*x^3 + C(2)*x^2*y + C(3)*x^2 + C(4)*x*y^2 + C(5)*x*y ...
            +C(6)*x + C(7)*y^3 + C(8)*y^2 + C(9)*y + C(10);
        return
    end

    %GLYL and GLYS
    function [z] = glyl_glys(x, y)
        C = [0.0153   -0.0003   -0.5475   -0.0000    0.0104    6.2328   ...
            -0.0000    0.0003   -0.1154  -13.4400];
        z = C(1)*x^3 + C(2)*x^2*y + C(3)*x^2 + C(4)*x*y^2 + C(5)*x*y ...
            +C(6)*x + C(7)*y^3 + C(8)*y^2 + C(9)*y + C(10);
        return
    end

end
